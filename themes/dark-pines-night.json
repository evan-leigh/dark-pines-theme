{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark Pines Night",
  "include": "./workbench.json",

  // Funcional Colors
  // Yellow Colors - PANTONE P 10-11 U
  // #FDD072E6 +34 Saturation
  // #E6CB92E6
  // #E6CB92E633 24% Opactity

  // Green Colors - PANTONE P 146-1 C
  // #A2C1891F +12 Saturation 12% Opacity
  // #A3B694E6

  // Blue Colors - PANTONE 2163 U
  // #98A1A9E6
  // #879CB0E6 +12 Saturation -2 Lightness

  // Red Colors - PANTONE 4069 UP
  // #B26C72E6
  // #AF5059E6 +6 Saturation -6 Lightness
  // #B26C72E61F +6 Saturation -6 Lightness 12% Opactiy

  // Orange Colors
  // #D08770E6

  // Highlight Colors
  // Light Colors - PANTONE 427 U, PANTONE 11-0601 TCX
  // #C2C7CAE6
  //
  // #D7DBDEE6

  // Dark Colors - PANTONE 426 XGC -4 lightness
  // #383D43E6 +14 Lightness
  // #33383DE6 +12 Lightness
  // #2A2E32E6 +8 Lightness
  // #1F2123E6 +4 Lightness
  // #1A1C1E
  // #161718 -2 Lightness -2 Saturation

  // Purple Colors - PANTONE 16-3310 TPG
  // #AA9AA8E6 Lavender Herb

  "tokenColors": [
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#B26C72F0"
      }
    },

    {
      "name": "Variables",
      "scope": "support.variable",
      "settings": {
        "foreground": "#B26C72F0"
      }
    },

    {
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#B26C72F0"
      }
    },

    {
      "name": "HTML Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#B26C72F0",
        "fontStyle": ""
      }
    },

    {
      "name": "Constants",
      "scope": "support.constant",
      "settings": {
        "foreground": "#98A1A9F0"
      }
    },

    {
      "name": "Functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#98A1A9F0"
      }
    },

    {
      "name": "Functions",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#98A1A9F0"
      }
    },

    {
      "scope": "constant.character",
      "settings": {
        "foreground": "#98A1A9F0"
      }
    },

    {
      "scope": "meta.object-literal",
      "settings": {
        "foreground": "#C2C7CAF0"
      }
    },

    {
      "scope": "entity.name",
      "settings": {
        "foreground": "#E6CB92F0"
      }
    },

    {
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#E6CB92F0"
      }
    },

    {
      "scope": "entity.name",
      "settings": {
        "foreground": "#E6CB92F0"
      }
    },

    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#E6CB92F0"
      }
    },

    {
      "scope": "support.class",
      "settings": {
        "foreground": "#E6CB92F0"
      }
    },

    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#FF0000"
      }
    },

    {
      "scope": "markup.heading, markup.bold",
      "settings": {
        "foreground": "#E6CB92F0"
      }
    },

    {
      "scope": "markup.fenced_code.block.markdown",
      "settings": {
        "foreground": "#98A1A9F0"
      }
    },

    {
      "scope": "markup.underline",
      "settings": {
        "foreground": "#879CB0F0"
      }
    },

    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#B26C72F0"
      }
    },

    {
      "scope": "support.type",
      "settings": {
        "foreground": "#98A1A9F0"
      }
    },

    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#D08770F0"
      }
    },

    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#D08770F0"
      }
    },

    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#98A1A9F0"
      }
    },

    {
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#D08770F0"
      }
    },

    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#C2C7CAF0"
      }
    },

    {
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#B26C72F0"
      }
    },

    {
      "scope": "variable.other.object.property",
      "settings": {
        "foreground": "#C2C7CAF0"
      }
    },

    {
      "scope": "comment.line, comment.block",
      "settings": {
        "foreground": "#7E868A"
      }
    },

    {
      "scope": "meta.resultLinePrefix",
      "settings": {
        "foreground": "#0000"
      }
    },

    {
      "scope": "meta.path",
      "settings": {
        "foreground": "#7E868A"
      }
    },

    {
      "scope": "meta.resultLinePrefix.lineNumber",
      "settings": {
        "foreground": "#C2C7CAF0"
      }
    },

    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#7E868A"
      }
    },

    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#AA9AA8F0",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#AA9AA8F0"
      }
    },

    {
      "scope": "storage.type.function.arrow",
      "settings": {
        "foreground": "#AA9AA8F0",
        "fontStyle": ""
      }
    },

    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#AA9AA8F0",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "keyword.control.flow",
      "settings": {
        "foreground": "#AA9AA8F0",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#AA9AA8F0",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#AA9AA8F0",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "keyword.control.conditional",
      "settings": {
        "foreground": "#AA9AA8F0",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "entity.other.attribute-name.pseudo-element",
      "settings": {
        "foreground": "#AA9AA8F0",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "keyword.other.important",
      "settings": {
        "foreground": "#E6CB92F0"
      }
    },

    {
      "scope": "keyword.control.import, keyword.control.export",
      "settings": {
        "foreground": "#AA9AA8F0",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "keyword.other",
      "settings": {
        "foreground": "#AA9AA8F0",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "constant.other.character-class",
      "settings": {
        "foreground": "#98A1A9F0"
      }
    },

    {
      "scope": "string",
      "settings": {
        "foreground": "#A3B694F0"
      }
    },

    {
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#C2C7CAF0"
      }
    }
  ],

  "colors": {
    // Editor
    "editor.background": "#1A1C1E",
    "editor.foreground": "#C2C7CAF0",

    "editor.rangeHighlightBackground": "#1F2123F0",

    "editor.findMatchBackground": "#383D43F0",
    "minimap.findMatchHighlight": "#383D43F0",
    "editorWhitespace.foreground": "#383D43F0",

    "editor.findMatchHighlightBackground": "#2A2E32F0",
    "editor.wordHighlightBackground": "#2A2E32F0",
    "editor.wordHighlightStrongBackground": "#2A2E32F0",
    "editor.hoverHighlightBackground": "#2A2E32F0",
    "editor.selectionBackground": "#2A2E32F0",

    // Sidebar
    "sideBar.background": "#161718",
    "sideBarSectionHeader.background": "#161718",

    // Scrollbar
    "scrollbarSlider.background": "#2A2E32F0",
    "scrollbarSlider.activeBackground": "#383D43F0",
    "scrollbarSlider.hoverBackground": "#383D43F0",

    // Minimap
    "minimap.background": "#1A1C1E",

    "minimap.selectionHighlight": "#383D4340",

    "minimapSlider.background": "#B2B2B212",
    "minimapSlider.hoverBackground": "#D9D9D912",
    "minimapSlider.activeBackground": "#ffffff12",

    // Highlights
    "editor.lineHighlightBackground": "#1F2123F0",
    "editorBracketMatch.border": "#7E868A",

    // Cursor
    "editorCursor.background": "#161718",
    "editorCursor.foreground": "#C2C7CAF0",

    // Line Numbers
    "editorLineNumber.foreground": "#7E868A",
    "editorLineNumber.activeForeground": "#C2C7CAF0",

    // Title Bar
    "titleBar.inactiveBackground": "#161718",
    "titleBar.activeBackground": "#161718",
    "titleBar.activeForeground": "#7E868A",
    "titleBar.inactiveForeground": "#7E868A",

    // Activity Bar
    "activityBar.background": "#161718",
    "activityBar.foreground": "#C2C7CAF0",

    // Tabs
    "tab.inactiveBackground": "#161718",
    "editorGroupHeader.tabsBackground": "#161718",
    "tab.activeForeground": "#C2C7CAF0",
    "tab.inactiveForeground": "#7E868A",
    "tab.activeBackground": "#1A1C1E",
    "tab.hoverBackground": "#1A1C1E",

    // Status Bar
    "statusBarItem.remoteBackground": "#00000080",
    "statusBarItem.remoteForeground": "#C2C7CAF0",
    "statusBar.background": "#161718",
    "statusBar.noFolderBackground": "#161718",
    "statusBar.foreground": "#C2C7CAF0",

    // Widgets
    "editorWidget.background": "#161718",
    "editorWidget.border": "#AF5059F0",
    "editorSuggestWidget.background": "#161718",
    "editorSuggestWidget.highlightForeground": "#B26C72F0",
    "editorSuggestWidget.foreground": "#C2C7CAF0",
    "editorSuggestWidget.focusHighlightForeground": "#B26C72F0",
    "menu.background": "#161718",

    // Banner
    "banner.background": "#161718",
    "banner.foreground": "#C2C7CAF0",

    // Debug
    "statusBar.debuggingBackground": "#1F2123F0",
    "debugToolBar.background": "#161718",

    // Breadcrumps
    "breadcrumb.background": "#1A1C1E",
    "breadcrumb.activeSelectionForeground": "#D7DBDEF0",

    // Buttons
    "button.background": "#AF5059F0",
    "button.foreground": "#D7DBDEF0",
    "activityBarBadge.background": "#AF5059F0",
    "activityBarBadge.foreground": "#D7DBDEF0",
    "button.secondaryBackground": "#2A2E32F0",
    "button.secondaryForeground": "#C2C7CAF0",
    "badge.background": "#AF5059F0",
    "badge.foreground": "#D7DBDEF0",
    "inputOption.activeBackground": "#2A2E32F0",
    "progressBar.background": "#B26C72F0",
    "merge.incomingHeaderBackground": "#B26C72F0",

    // Lists
    "dropdown.background": "#32353B",
    "input.background": "#161718",

    "list.focusBackground": "#1F2123F0",

    "quickInputTitle.background": "#1F2123F0",

    "list.hoverBackground": "#1F2123F0",
    "dropdown.border": "#2A2E32F0",
    "list.activeSelectionBackground": "#1F2123F0",

    "list.highlightForeground": "#B26C72F0",
    "list.focusHighlightForeground": "#B26C72F0",

    // Peek View
    "peekViewResult.background": "#161718",
    "peekViewTitle.background": "#161718",
    "peekViewEditor.background": "#1A1C1E",
    "peekView.border": "#2A2E32F0",
    "peekViewResult.selectionBackground": "#1A1C1E",
    "peekViewEditor.matchHighlightBackground": "#2A2E32F0",
    "peekViewResult.matchHighlightBackground": "#2A2E32F0",

    // Git
    "editorGutter.modifiedBackground": "#879CB0F0",
    "editorGutter.addedBackground": "#A3B694F0",
    "editorGutter.deletedBackground": "#B26C72F0",

    // Git Decorations
    "gitDecoration.modifiedResourceForeground": "#E6CB92F0",
    "gitDecoration.stageModifiedResourceForeground": "#E6CB92F0",
    "gitDecoration.addedResourceForeground": "#A3B694F0",
    "gitDecoration.untrackedResourceForeground": "#A3B694F0",
    "minimapGutter.addedBackground": "#A3B694F0",
    "minimapGutter.modifiedBackground": "#879CB0F0",
    "minimapGutter.deletedBackground": "#B26C72F0",

    // Errors
    "minimap.errorHighlight": "#AF5059F0",
    "editorOverviewRuler.errorForeground": "#AF5059F0",
    "problemsErrorIcon.foreground": "#AF5059F0",
    "list.errorForeground": "#B26C72F0",
    "editorError.foreground": "#B26C72F0",

    // Warning
    "list.warningForeground": "#C4B969",
    "editorWarning.foreground": "#E6CB92F0",
    "minimap.warningHighlight": "#E6CB92F0",
    "editorOverviewRuler.warningForeground": "#E6CB92F0",

    // Difference
    "diffEditor.border": "#2A2E32F0",
    "diffEditor.insertedTextBackground": "#A2BD8D1F",
    "diffEditor.removedTextBackground": "#B26C722E",

    // Terminal
    "terminal.ansiRed": "#B26C72F0",
    "terminal.ansiBrightRed": "#AF5059F0",
    "terminal.ansiCyan": "#98A1A9F0",
    "terminal.ansiGreen": "#A3B694F0",
    "terminal.ansiBrightWhite": "#D7DBDEF0",
    "terminal.ansiWhite": "#C2C7CAF0",
    "terminal.ansiBrightYellow": "#FDD072F0",
    "terminal.ansiYellow": "#E6CB92F0",
    "terminal.ansiMagenta": "#AA9AA8F0"
  }
}
