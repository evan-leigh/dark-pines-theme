{
  "$schema": "vscode://schemas/color-theme",
  "name": "Meadows",
  "include": "./main.json",

  // #D8D8D8
  // #C4B969 -3 lightness

  // #CCCCCC

  // #E5E5E5
  // #E5E5E533 25% Opacity
  // #E5E5E524 14% Opacity
  // #E5E5E517 7% Opacity

  // #BFBFBF +3 Lightness
  // #BFBFBF

  // #B2B2B2

  // #2C303A +2 Lightness
  // #282B34
  // #21242B -3 Lightness

  // #8F9699 +6 Lightness
  // #7E868A

  // #E7E9E7
  // #CCCCC7 -12 Lightness

  // #A6A6A6

  "tokenColors": [
    {
      "scope": "variable",
      "settings": {
        "foreground": "#B2B2B2"
      }
    },
    {
      "scope": "entity.name",
      "settings": {
        "foreground": "#B2B2B2"
      }
    },
    {
      "scope": "meta.object-literal",
      "settings": {
        "foreground": "#B2B2B2"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#A6A6A6"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#A6A6A6"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#A6A6A6"
      }
    },
    {
      "scope": "keyword.control.import, keyword.control.export",
      "settings": {
        "foreground": "#A6A6A6"
      }
    },
    {
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#D8D8D8"
      }
    },
    {
      "scope": "entity.name",
      "settings": {
        "foreground": "#D8D8D8"
      }
    },
    {
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#D8D8D8"
      }
    },
    {
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#D8D8D8"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#D8D8D8"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#D8D8D8"
      }
    },
    {
      "scope": "constant.character",
      "settings": {
        "foreground": "#D8D8D8"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#E7E9E7"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#B2B2B2"
      }
    },
    {
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#B2B2B2"
      }
    },
    {
      "scope": "comment.line",
      "settings": {
        "foreground": "#7E868A"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#7E868A"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#BFBFBF"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#BFBFBF"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#BFBFBF"
      }
    },
    {
      "scope": "keyword.control.flow",
      "settings": {
        "foreground": "#BFBFBF"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#BFBFBF"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#BFBFBF"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#BFBFBF"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#BFBFBF"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#BFBFBF"
      }
    },
    {
      "scope": "keyword.control.conditional",
      "settings": {
        "foreground": "#BFBFBF"
      }
    },
    {
      "scope": "keyword.other",
      "settings": {
        "foreground": "#BFBFBF"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#CCCCC7"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#CCCCCC"
      }
    }
  ],

  "colors": {
    // Editor
    "editor.background": "#2C303A",
    "editor.foreground": "#CCCCC7",
    "editor.findMatchBackground": "#E5E5E533",
    "editor.rangeHighlightBackground": "#2C303A",
    "editor.findMatchHighlightBackground": "#E5E5E517",
    "editor.wordHighlightBackground": "#E5E5E517",
    "editor.wordHighlightStrongBackground": "#E5E5E517",
    "editor.hoverHighlightBackground": "#E5E5E517",
    "editor.selectionBackground": "#E5E5E517",
    "editorWhitespace.foreground": "#E5E5E533",

    // Sidebar
    "sideBar.background": "#21242B",
    "sideBarSectionHeader.background": "#21242B",

    // Scrollbar
    "scrollbarSlider.background": "#E5E5E517",
    "scrollbarSlider.activeBackground": "#E5E5E533",
    "scrollbarSlider.hoverBackground": "#E5E5E533",

    // Minimap
    "minimap.selectionHighlight": "#E5E5E533",
    "minimap.background": "#2C303A",
    "minimapSlider.background": "#E5E5E517",
    "minimapSlider.hoverBackground": "#E5E5E524",
    "minimapSlider.activeBackground": "#E5E5E533",

    "editorWidget.border": "#B2B2B2",

    "editor.lineHighlightBackground": "#2C303A",
    "editorBracketMatch.border": "#7E868A",

    "editorCursor.background": "#E7E9E7",
    "editorCursor.foreground": "#B2B2B2",
    "editorLineNumber.foreground": "#7E868A",
    "editorLineNumber.activeForeground": "#CCCCC7",

    // Title Bar
    "titleBar.inactiveBackground": "#21242B",
    "titleBar.activeBackground": "#21242B",

    // Activity Bar
    "activityBar.background": "#21242B",
    "activityBar.foreground": "#CCCCC7",

    // Tabs
    "tab.inactiveBackground": "#21242B",
    "editorGroupHeader.tabsBackground": "#21242B",

    // Status Bar
    "statusBarItem.remoteBackground": "#BFBFBF",
    "statusBarItem.remoteForeground": "#21242B",
    "statusBar.background": "#21242B",
    "statusBar.foreground": "#CCCCC7",

    // Widgets
    "editorWidget.background": "#21242B",
    "editorSuggestWidget.background": "#21242B",
    "editorSuggestWidget.highlightForeground": "#BFBFBF",
    "editorSuggestWidget.foreground": "#7E868A",

    // Debug
    "statusBar.debuggingBackground": "#2C303A",
    "debugToolBar.background": "#21242B",

    // Breadcrumps
    "breadcrumb.background": "#21242B",

    // Buttons
    "button.background": "#BFBFBF",
    "button.foreground": "#21242B",
    "activityBarBadge.background": "#BFBFBF",
    "activityBarBadge.foreground": "#21242B",
    "button.secondaryBackground": "#7E868A",
    "button.secondaryForeground": "#E7E9E7",
    "badge.background": "#BFBFBF",
    "badge.foreground": "#30343F",

    // Lists
    "dropdown.background": "#21242B",
    "input.background": "#21242B",
    "list.focusBackground": "#30343F",
    "list.hoverBackground": "#30343F",
    "list.activeSelectionBackground": "#2C303A",
    "list.highlightForeground": "#BFBFBF",
    "list.warningForeground": "#C4B969",
    "list.focusHighlightForeground": "#CCCCC7",

    // Peek View
    "peekViewResult.background": "#21242B",
    "peekViewTitle.background": "#21242B",
    "peekViewEditor.background": "#282B34",
    "peekViewResult.selectionBackground": "#282B34",
    "peekViewEditor.matchHighlightBackground": "#E5E5E533",
    "peekViewResult.matchHighlightBackground": "#E5E5E533",

    // Git
    "editorGutter.modifiedBackground": "#A6A6A6",
    "editorGutter.addedBackground": "#CCCCCC",
    "editorGutter.deletedBackground": "#B2B2B2",

    "gitDecoration.modifiedResourceForeground": "#D8D8D8",
    "gitDecoration.stageModifiedResourceForeground": "#D8D8D8",
    "gitDecoration.addedResourceForeground": "#CCCCCC",
    "gitDecoration.untrackedResourceForeground": "#CCCCCC",

    "editorOverviewRuler.warningForeground": "#D8D8D8",
    "editorWarning.foreground": "#C4B969",

    // Terminal
    "terminal.ansiRed": "#B2B2B2",
    "terminal.ansiCyan": "#A6A6A6",
    "terminal.ansiGreen": "#CCCCCC",
    "terminal.ansiWhite": "#E5E5E5",
    "terminal.ansiYellow": "#D8D8D8",
    "terminal.ansiMagenta": "#BFBFBF"
  }
}
