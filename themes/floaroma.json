{
  "$schema": "vscode://schemas/color-theme",
  "name": "Floaroma",
  "include": "./workbench.json",

  // PANTONE 13-0614 TPG
  // #D5CD94 Golden Mist
  // #C4B969 Golden Mist - 3 lightness

  // PANTONE 14-6319 TPG
  // #8DBB96 Meadow

  // PANTONE  12-5204 TCX
  // #98ADAE Morning Mist
  // #98ADAE33 Morning Mist 25% Opacity
  // #98ADAE24 Morning Mist 14% Opacity
  // #98ADAE17 Morning Mist 7% Opacity
  // #98ADAE0D Morning Mist 5% Opacity

  // PANTONE 15-1512 TCX
  // #B09FCA Mystic Violet +3 Lightness
  // #B09FCA Mystic Violet

  // PANTONE 17-1929 TCX
  // #DB8097 Rapture Rose

  // PANTONE  19-4011 TCX
  // #2C303A Salute +2 Lightness
  // #282B34 Salute
  // #23262E Salute -3 Lightness

  // PANTONE 19-4019 TPG
  // #8F9699 Monument +6 Lightness
  // #7E868A Monument

  // PANTONE 11-4800 TPG
  // #E7E9E7 Blanc De Blanc
  // #CCCCC7 Blanc De Blanc -12 Lightness

  // PANTONE  16-4421 TCX
  // #5BACC3 Blue Mist

  "tokenColors": [
    {
      "scope": "variable",
      "settings": {
        "foreground": "#DB8097"
      }
    },
    {
      "scope": "entity.name",
      "settings": {
        "foreground": "#DB8097"
      }
    },
    {
      "scope": "meta.object-literal",
      "settings": {
        "foreground": "#DB8097"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#5BACC3"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#5BACC3"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#5BACC3"
      }
    },
    {
      "scope": "keyword.control.import, keyword.control.export",
      "settings": {
        "foreground": "#5BACC3"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#D5CD94"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#D5CD94"
      }
    },
    {
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#D5CD94"
      }
    },
    {
      "scope": "entity.name",
      "settings": {
        "foreground": "#D5CD94"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#D5CD94"
      }
    },
    {
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#D5CD94"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#D5CD94"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#D5CD94"
      }
    },
    {
      "scope": "constant.character",
      "settings": {
        "foreground": "#D5CD94"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#E7E9E7"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#DB8097"
      }
    },
    {
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#DB8097"
      }
    },
    {
      "scope": "comment.line, comment.block",
      "settings": {
        "foreground": "#7E868A"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#7E868A"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#B09FCA"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#B09FCA"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#B09FCA"
      }
    },
    {
      "scope": "keyword.control.flow",
      "settings": {
        "foreground": "#B09FCA"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#B09FCA"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#B09FCA"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#B09FCA"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#B09FCA"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#B09FCA"
      }
    },
    {
      "scope": "keyword.control.conditional",
      "settings": {
        "foreground": "#B09FCA"
      }
    },
    {
      "scope": "keyword.other",
      "settings": {
        "foreground": "#B09FCA"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#CCCCC7"
      }
    },
    {
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#CCCCC7"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#8DBB96"
      }
    }
  ],

  "colors": {
    // Editor
    "editor.background": "#2C303A",
    "editor.foreground": "#CCCCC7",
    "editor.findMatchBackground": "#98ADAE33",
    "editor.rangeHighlightBackground": "#2C303A",
    "editor.findMatchHighlightBackground": "#98ADAE17",
    "editor.wordHighlightBackground": "#98ADAE17",
    "editor.wordHighlightStrongBackground": "#98ADAE0D",
    "editor.hoverHighlightBackground": "#98ADAE17",
    "editor.selectionBackground": "#98ADAE17",
    "editorWhitespace.foreground": "#98ADAE33",

    "editorError.foreground": "#DB8097",
    "editorWarning.foreground": "#D5CD94",

    // Sidebar
    "sideBar.background": "#23262E",
    "sideBarSectionHeader.background": "#23262E",

    // Scrollbar
    "scrollbarSlider.background": "#98ADAE17",
    "scrollbarSlider.activeBackground": "#98ADAE33",
    "scrollbarSlider.hoverBackground": "#98ADAE33",

    // Minimap
    "minimap.selectionHighlight": "#98ADAE33",
    "minimap.background": "#2C303A",
    "minimapSlider.background": "#98ADAE17",
    "minimapSlider.hoverBackground": "#98ADAE24",
    "minimapSlider.activeBackground": "#98ADAE33",

    "editorWidget.border": "#DB8097",

    "editor.lineHighlightBackground": "#2C303A",
    "editorBracketMatch.border": "#7E868A",

    "editorCursor.background": "#E7E9E7",
    "editorCursor.foreground": "#DB8097",
    "editorLineNumber.foreground": "#7E868A",
    "editorLineNumber.activeForeground": "#CCCCC7",

    // Title Bar
    "titleBar.inactiveBackground": "#23262E",
    "titleBar.activeBackground": "#23262E",

    // Activity Bar
    "activityBar.background": "#23262E",
    "activityBar.foreground": "#CCCCC7",

    // Tabs
    "tab.inactiveBackground": "#23262E",
    "editorGroupHeader.tabsBackground": "#23262E",

    // Status Bar
    "statusBarItem.remoteBackground": "#B09FCA",
    "statusBarItem.remoteForeground": "#23262E",
    "statusBar.background": "#23262E",
    "statusBar.foreground": "#CCCCC7",
    "statusBar.noFolderBackground": "#23262E",

    // Widgets
    "editorWidget.background": "#23262E",
    "editorSuggestWidget.background": "#23262E",
    "editorSuggestWidget.highlightForeground": "#B09FCA",
    "editorSuggestWidget.foreground": "#7E868A",

    // Debug
    "statusBar.debuggingBackground": "#2C303A",
    "debugToolBar.background": "#23262E",

    // Breadcrumps
    "breadcrumb.background": "#2C303A",

    // Buttons
    "button.background": "#B09FCA",
    "button.foreground": "#23262E",
    "activityBarBadge.background": "#B09FCA",
    "activityBarBadge.foreground": "#23262E",
    "button.secondaryBackground": "#7E868A",
    "button.secondaryForeground": "#E7E9E7",
    "badge.background": "#B09FCA",
    "badge.foreground": "#30343F",

    // Lists
    "dropdown.background": "#23262E",
    "input.background": "#23262E",
    "list.focusBackground": "#30343F",
    "list.hoverBackground": "#30343F",
    "list.activeSelectionBackground": "#2C303A",
    "list.highlightForeground": "#B09FCA",
    "list.warningForeground": "#C4B969",
    "list.focusHighlightForeground": "#CCCCC7",

    // Peek View
    "peekViewResult.background": "#23262E",
    "peekViewTitle.background": "#23262E",
    "peekViewEditor.background": "#282B34",
    "peekViewResult.selectionBackground": "#282B34",
    "peekViewEditor.matchHighlightBackground": "#98ADAE33",
    "peekViewResult.matchHighlightBackground": "#98ADAE33",

    // Git
    "editorGutter.modifiedBackground": "#5BACC3",
    "editorGutter.addedBackground": "#8DBB96",
    "editorGutter.deletedBackground": "#DB8097",

    "gitDecoration.modifiedResourceForeground": "#D5CD94",
    "gitDecoration.stageModifiedResourceForeground": "#D5CD94",
    "gitDecoration.addedResourceForeground": "#8DBB96",
    "gitDecoration.untrackedResourceForeground": "#8DBB96",

    "editorOverviewRuler.warningForeground": "#D5CD94",

    // Terminal
    "terminal.ansiRed": "#DB8097",
    "terminal.ansiCyan": "#5BACC3",
    "terminal.ansiGreen": "#8DBB96",
    "terminal.ansiWhite": "#98ADAE",
    "terminal.ansiYellow": "#D5CD94",
    "terminal.ansiMagenta": "#B09FCA"
  }
}
