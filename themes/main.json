{
  "$schema": "vscode://schemas/color-theme",
  "name": "",
  "include": "./editor.json",

  "tokenColors": [
    {
      "name": "Pointer, access, etc",
      "scope": "meta.ptr, meta.pointer, meta.address, meta.array.cxx",
      "settings": {
        "foreground": "#EBA96C"
      }
    },

    {
      "name": "diff.header",
      "scope": "meta.diff, meta.diff.header",
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#F85931"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#E6DB74"
      }
    },

    {
      "name": "Markdown Raw",
      "scope": "markup.raw, markup.inline.raw, markup.fenced, markup.fenced_code",
      "settings": {
        "foreground": "#cc7f66"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}
