{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark Pines",
  "include": "./workbench.json",

  // Funcional Colors
  // Yellow Colors - PANTONE P 10-11 U
  // #FDD072 +34 Saturation
  // #E6CB92
  // #E6CB9233 24% Opactity

  // Green Colors - PANTONE P 146-1 C
  // #A2C1891F +12 Saturation 12% Opacity
  // #A3B694

  // Blue Colors - PANTONE 2163 U
  // #98A1A9
  // #879CB0 +12 Saturation -2 Lightness

  // Red Colors - PANTONE 4069 UP
  // #B26C72
  // #AF5059 +6 Saturation -6 Lightness
  // #B26C721F +6 Saturation -6 Lightness 12% Opactiy

  // Orange Colors
  // #D08770

  // Highlight Colors
  // Light Colors - PANTONE 427 U, PANTONE 11-0601 TCX
  // #C2C7CA
  //
  // #f4f9ff

  // Dark Colors - PANTONE 426 XGC
  // #41474E +14 Lightness
  // #383D43 +12 Lightness
  // #33383D +8 Lightness
  // #2A2E32 +4 Lightness
  // #24282B
  // #1F2123 -2 Lightness -2 Saturation

  // Purple Colors - PANTONE 16-3310 TPG
  // #AA9AA8 Lavender Herb

  // Transparent Colors
  // #383D43CC
  // #33383DCC
  // #2A2E32CC

  "tokenColors": [
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#B26C72"
      }
    },

    {
      "name": "Variables",
      "scope": "support.variable",
      "settings": {
        "foreground": "#B26C72"
      }
    },

    {
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#B26C72"
      }
    },

    {
      "name": "HTML Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#B26C72"
      }
    },

    {
      "name": "Constants",
      "scope": "support.constant",
      "settings": {
        "foreground": "#98A1A9"
      }
    },

    {
      "name": "Functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#98A1A9"
      }
    },

    {
      "name": "Functions",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#98A1A9"
      }
    },

    {
      "scope": "constant.character",
      "settings": {
        "foreground": "#98A1A9"
      }
    },

    {
      "scope": "meta.object-literal",
      "settings": {
        "foreground": "#C2C7CA"
      }
    },

    {
      "scope": "entity.name",
      "settings": {
        "foreground": "#E6CB92"
      }
    },

    {
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#E6CB92"
      }
    },

    {
      "scope": "entity.name",
      "settings": {
        "foreground": "#E6CB92"
      }
    },

    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#E6CB92"
      }
    },

    {
      "scope": "support.class",
      "settings": {
        "foreground": "#E6CB92"
      }
    },

    {
      "scope": "markup.heading, markup.bold",
      "settings": {
        "foreground": "#E6CB92"
      }
    },

    {
      "scope": "markup.fenced_code.block.markdown",
      "settings": {
        "foreground": "#98A1A9"
      }
    },

    {
      "scope": "markup.underline",
      "settings": {
        "foreground": "#879CB0"
      }
    },

    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#B26C72"
      }
    },

    {
      "scope": "support.type",
      "settings": {
        "foreground": "#98A1A9"
      }
    },

    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#D08770"
      }
    },

    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#D08770"
      }
    },

    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#98A1A9"
      }
    },

    {
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#D08770"
      }
    },

    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#C2C7CA"
      }
    },

    {
      "scope": "variable.other.object.property",
      "settings": {
        "foreground": "#C2C7CA"
      }
    },

    {
      "scope": "comment.line, comment.block",
      "settings": {
        "foreground": "#7E868A"
      }
    },

    {
      "scope": "meta.resultLinePrefix",
      "settings": {
        "foreground": "#0000"
      }
    },

    {
      "scope": "meta.path",
      "settings": {
        "foreground": "#7E868A"
      }
    },

    {
      "scope": "meta.resultLinePrefix.lineNumber",
      "settings": {
        "foreground": "#C2C7CA"
      }
    },

    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#7E868A"
      }
    },

    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#AA9AA8",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#AA9AA8"
      }
    },

    {
      "scope": "storage.type.function.arrow",
      "settings": {
        "foreground": "#AA9AA8",
        "fontStyle": ""
      }
    },

    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#AA9AA8",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "keyword.control.flow",
      "settings": {
        "foreground": "#AA9AA8",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#AA9AA8",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#AA9AA8",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "keyword.control.conditional",
      "settings": {
        "foreground": "#AA9AA8",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "entity.other.attribute-name.pseudo-element",
      "settings": {
        "foreground": "#AA9AA8",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "keyword.other.important",
      "settings": {
        "foreground": "#E6CB92"
      }
    },

    {
      "scope": "keyword.control.import, keyword.control.export",
      "settings": {
        "foreground": "#AA9AA8",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "keyword.other",
      "settings": {
        "foreground": "#AA9AA8",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "constant.other.character-class",
      "settings": {
        "foreground": "#98A1A9"
      }
    },

    {
      "scope": "string",
      "settings": {
        "foreground": "#A3B694"
      }
    },

    {
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#C2C7CA"
      }
    }
  ],

  "colors": {
    // Editor
    "editor.background": "#24282B",
    "editor.foreground": "#C2C7CA",

    "editor.rangeHighlightBackground": "#2A2E32",

    "editor.findMatchBackground": "#41474E",
    "minimap.findMatchHighlight": "#41474E",
    "editorWhitespace.foreground": "#41474E",

    "editor.findMatchHighlightBackground": "#33383D",
    "editor.wordHighlightBackground": "#33383D",
    "editor.wordHighlightStrongBackground": "#33383D",
    "editor.hoverHighlightBackground": "#33383D",
    "editor.selectionBackground": "#33383D",

    // Sidebar
    "sideBar.background": "#1F2123",
    "sideBarSectionHeader.background": "#1F2123",

    // Minimap
    "minimap.selectionHighlight": "#41474E40",

    "minimap.background": "#24282B",

    "minimapSlider.background": "#C2C2C212",
    "minimapSlider.hoverBackground": "#D9D9D912",
    "minimapSlider.activeBackground": "#F2F2F212",

    // Highlights
    "editor.lineHighlightBackground": "#2A2E32",
    "editorBracketMatch.border": "#7E868A",

    // Cursor
    "editorCursor.background": "#C2C7CA",
    "editorCursor.foreground": "#B26C72",

    // Line Numbers
    "editorLineNumber.foreground": "#7E868A",
    "editorLineNumber.activeForeground": "#C2C7CA",

    // Title Bar
    "titleBar.inactiveBackground": "#1F2123",
    "titleBar.activeBackground": "#1F2123",
    "titleBar.activeForeground": "#7E868A",
    "titleBar.inactiveForeground": "#7E868A",

    // Activity Bar
    "activityBar.background": "#1F2123",
    "activityBar.foreground": "#C2C7CA",

    // Tabs
    "tab.inactiveBackground": "#1F2123",
    "editorGroupHeader.tabsBackground": "#1F2123",
    "tab.activeForeground": "#c2c7ca",
    "tab.inactiveForeground": "#7E868A",
    "tab.activeBackground": "#24282B",
    "tab.hoverBackground": "#24282B",

    // Status Bar
    "statusBarItem.remoteBackground": "#00000080",
    "statusBarItem.remoteForeground": "#c2c7ca",
    "statusBar.background": "#1F2123",
    "statusBar.noFolderBackground": "#1F2123",
    "statusBar.foreground": "#C2C7CA",

    // Widgets
    "editorWidget.background": "#1F2123",
    "editorWidget.border": "#B26C72",
    "editorSuggestWidget.background": "#1F2123",
    "editorSuggestWidget.highlightForeground": "#B26C72",
    "editorSuggestWidget.foreground": "#C2C7CA",
    "editorSuggestWidget.focusHighlightForeground": "#B26C72",
    "menu.background": "#1F2123",

    // Banner
    "banner.background": "#1F2123",
    "banner.foreground": "#C2C7CA",

    // Debug
    "statusBar.debuggingBackground": "#2A2E32",
    "debugToolBar.background": "#1F2123",

    // Breadcrumps
    "breadcrumb.background": "#24282B",
    "breadcrumb.activeSelectionForeground": "#f4f9ff",

    // Buttons
    "button.background": "#AF5059",
    "button.foreground": "#f4f9ff",
    "activityBarBadge.background": "#AF5059",
    "activityBarBadge.foreground": "#f4f9ff",
    "button.secondaryBackground": "#33383D",
    "button.secondaryForeground": "#C2C7CA",
    "badge.background": "#AF5059",
    "badge.foreground": "#f4f9ff",
    "inputOption.activeBackground": "#33383D",
    "progressBar.background": "#B26C72",
    "merge.incomingHeaderBackground": "#B26C72",

    // Lists
    "dropdown.background": "#32353B",
    "input.background": "#1F2123",

    "list.focusBackground": "#2A2E32",

    "quickInputTitle.background": "#2A2E32",

    "list.hoverBackground": "#2A2E32",
    "dropdown.border": "#33383D",
    "list.activeSelectionBackground": "#2A2E32",

    "list.highlightForeground": "#B26C72",
    "list.focusHighlightForeground": "#B26C72",

    // Peek View
    "peekViewResult.background": "#1F2123",
    "peekViewTitle.background": "#1F2123",
    "peekViewEditor.background": "#24282B",
    "peekView.border": "#33383D",
    "peekViewResult.selectionBackground": "#24282B",
    "peekViewEditor.matchHighlightBackground": "#33383D",
    "peekViewResult.matchHighlightBackground": "#33383D",

    // Git
    "editorGutter.modifiedBackground": "#879CB0",
    "editorGutter.addedBackground": "#A3B694",
    "editorGutter.deletedBackground": "#B26C72",

    // Git Decorations
    "gitDecoration.modifiedResourceForeground": "#E6CB92",
    "gitDecoration.stageModifiedResourceForeground": "#E6CB92",
    "gitDecoration.addedResourceForeground": "#A3B694",
    "gitDecoration.untrackedResourceForeground": "#A3B694",
    "minimapGutter.addedBackground": "#A3B694",
    "minimapGutter.modifiedBackground": "#879CB0",
    "minimapGutter.deletedBackground": "#B26C72",

    // Errors
    "minimap.errorHighlight": "#AF5059",
    "editorOverviewRuler.errorForeground": "#AF5059",
    "problemsErrorIcon.foreground": "#AF5059",
    "list.errorForeground": "#B26C72",
    "editorError.foreground": "#B26C72",

    // Warning
    "list.warningForeground": "#C4B969",
    "editorWarning.foreground": "#E6CB92",
    "minimap.warningHighlight": "#E6CB9233",
    "editorOverviewRuler.warningForeground": "#E6CB92",

    // Difference
    "diffEditor.border": "#33383D",
    "diffEditor.insertedTextBackground": "#A2BD8D1F",
    "diffEditor.removedTextBackground": "#B26C722E",

    // Terminal
    "terminal.ansiRed": "#B26C72",
    "terminal.ansiBrightRed": "#AF5059",
    "terminal.ansiCyan": "#98A1A9",
    "terminal.ansiGreen": "#A3B694",
    "terminal.ansiBrightWhite": "#f4f9ff",
    "terminal.ansiWhite": "#C2C7CA",
    "terminal.ansiBrightYellow": "#FDD072",
    "terminal.ansiYellow": "#E6CB92",
    "terminal.ansiMagenta": "#AA9AA8"
  }
}
